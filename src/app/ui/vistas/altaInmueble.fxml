<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<HBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="650.0" prefWidth="1000.0" stylesheets="@../estilos/style.css"
      xmlns="http://javafx.com/javafx/8.0.65" fx:controller="app.ui.controladores.BaseController">
    <children>
        <VBox alignment="TOP_CENTER" maxWidth="80.0" minWidth="80.0" prefWidth="80.0" spacing="10.0"
              styleClass="sidebar">
            <children>
                <VBox alignment="CENTER">
                    <children>
                        <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true"
                                   VBox.vgrow="SOMETIMES">
                            <image>
                                <Image url="@../../../../res/img/icono-w-64.png"/>
                            </image>
                        </ImageView>
                        <Label alignment="CENTER" styleClass="label-white-title" text="Olimpo"/>
                    </children>
                </VBox>
                <VBox alignment="CENTER">
                    <children>
                        <ToggleButton fx:id="toggleButtonClientes" mnemonicParsing="false"
                                      styleClass="toggle-button-cliente"/>
                        <Label alignment="CENTER" prefHeight="16.0" prefWidth="119.0" styleClass="label-white"
                               text="Clientes"/>
                    </children>
                </VBox>
                <VBox alignment="CENTER">
                    <children>
                        <ToggleButton fx:id="toggleButtonInmuebles" mnemonicParsing="false"
                                      styleClass="toggle-button-inmueble"/>
                        <Label alignment="CENTER" prefHeight="16.0" prefWidth="119.0" styleClass="label-white"
                               text="Inmuebles"/>
                    </children>
                </VBox>
                <VBox alignment="CENTER">
                    <children>
                        <ToggleButton fx:id="toggleButtonVendedores" mnemonicParsing="false"
                                      styleClass="toggle-button-vendedor"/>
                        <Label alignment="CENTER" prefHeight="16.0" prefWidth="119.0" styleClass="label-white"
                               text="Vendedores"/>
                    </children>
                </VBox>
                <VBox alignment="CENTER">
                    <children>
                        <ToggleButton fx:id="toggleButtonPropietarios" mnemonicParsing="false"
                                      styleClass="toggle-button-propietario"/>
                        <Label alignment="CENTER" prefHeight="16.0" prefWidth="119.0" styleClass="label-white"
                               text="Dueños"/>
                    </children>
                </VBox>
                <VBox alignment="CENTER">
                    <children>
                        <ToggleButton fx:id="toggleButtonAyuda" mnemonicParsing="false"
                                      styleClass="toggle-button-ayuda"/>
                        <Label alignment="CENTER" prefHeight="16.0" prefWidth="119.0" styleClass="label-white"
                               text="Ayuda"/>
                    </children>
                </VBox>
            </children>
            <HBox.margin>
                <Insets/>
            </HBox.margin>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </VBox>
        <VBox HBox.hgrow="ALWAYS">
            <children>
                <HBox fx:id="titlebar" alignment="CENTER_RIGHT" maxHeight="30.0" minHeight="30.0" prefHeight="30.0"
                      prefWidth="200.0">
                    <children>
                        <Label styleClass="label-large" text="Alta Inmueble">
                            <padding>
                                <Insets left="30.0" right="10.0" top="30.0"/>
                            </padding>
                        </Label>
                        <Pane prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS"/>
                        <ToggleButton fx:id="minimizeButton" mnemonicParsing="false" onAction="#minimizePlatform"
                                      styleClass="toggle-button-minimize"/>
                        <ToggleButton fx:id="maximizeButton" mnemonicParsing="false" onAction="#maximizePlatform"
                                      styleClass="toggle-button-maximize"/>
                        <ToggleButton fx:id="closeButton" mnemonicParsing="false" onAction="#exitPlatform"
                                      styleClass="toggle-button-close"/>
                    </children>
                </HBox>
                <VBox prefHeight="200.0" prefWidth="100.0" styleClass="main" VBox.vgrow="ALWAYS">
                    <children>
                        <GridPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                  minHeight="-Infinity" minWidth="-Infinity" styleClass="grid-pane"
                                  VBox.vgrow="SOMETIMES">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="30.0" prefWidth="50.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="500.0" minWidth="250.0"
                                                   prefWidth="300.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="30.0" prefWidth="50.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="500.0" minWidth="250.0"
                                                   prefWidth="300.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="30.0" prefWidth="50.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <children>
                                <TextField maxHeight="1000.0" maxWidth="1000.0" promptText="Código del inmueble"
                                           GridPane.columnIndex="1"/>
                                <TextField maxHeight="1000.0" maxWidth="1000.0" promptText="Fecha de carga"
                                           GridPane.columnIndex="3"/>
                                <ComboBox maxHeight="1000.0" maxWidth="1000.0" prefWidth="150.0" promptText="Provincia"
                                          GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                                <ComboBox maxHeight="1000.0" maxWidth="1000.0" prefWidth="150.0" promptText="Localidad"
                                          GridPane.columnIndex="3" GridPane.rowIndex="1"/>
                                <TextField maxHeight="1000.0" maxWidth="1000.0" promptText="Calle"
                                           GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                                <TextField maxHeight="1000.0" maxWidth="1000.0" promptText="Altura"
                                           GridPane.columnIndex="3" GridPane.rowIndex="2"/>
                                <TextField maxHeight="1000.0" maxWidth="1000.0" promptText="Piso"
                                           GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                                <TextField maxHeight="1000.0" maxWidth="1000.0" promptText="Departamento"
                                           GridPane.columnIndex="3" GridPane.rowIndex="3"/>
                                <TextField maxHeight="1000.0" maxWidth="1000.0" promptText="Barrio"
                                           GridPane.columnIndex="1" GridPane.rowIndex="4"/>
                                <ComboBox maxHeight="1000.0" maxWidth="1000.0" prefWidth="150.0"
                                          promptText="Tipo de Inmueble" GridPane.columnIndex="3" GridPane.rowIndex="4"/>
                                <TextField maxHeight="1000.0" maxWidth="1000.0" promptText="Precio de venta"
                                           GridPane.columnIndex="1" GridPane.rowIndex="5"/>
                                <ComboBox maxHeight="1000.0" maxWidth="1000.0" prefWidth="150.0"
                                          promptText="Orientación" GridPane.columnIndex="3" GridPane.rowIndex="5"/>
                            </children>
                            <padding>
                                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0"/>
                            </padding>
                        </GridPane>
                        <GridPane styleClass="grid-pane" VBox.vgrow="SOMETIMES">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="100.0" minWidth="100.0"
                                                   prefWidth="100.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="100.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="100.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="100.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="30.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <children>
                                <TextField maxHeight="1000.0" maxWidth="1000.0" promptText="Frente"
                                           GridPane.columnIndex="1"/>
                                <TextField maxHeight="1000.0" maxWidth="1000.0" promptText="Fondo"
                                           GridPane.columnIndex="2"/>
                                <TextField maxHeight="1000.0" maxWidth="1000.0" promptText="Superficie"
                                           GridPane.columnIndex="3"/>
                            </children>
                        </GridPane>
                        <GridPane styleClass="grid-pane" VBox.vgrow="SOMETIMES">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="100.0" minWidth="100.0"
                                                   prefWidth="100.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="200.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="30.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="200.0" minHeight="10.0" prefHeight="100.0"
                                                vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <children>
                                <TextField maxHeight="1000.0" maxWidth="1000.0" promptText="Fotos"
                                           GridPane.columnIndex="1"/>
                                <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="200.0"
                                        styleClass="button-action" text="Buscar Foto" GridPane.columnIndex="2"
                                        GridPane.halignment="CENTER" GridPane.valignment="CENTER"/>
                            </children>
                        </GridPane>
                    </children>
                    <padding>
                        <Insets top="30.0"/>
                    </padding>
                </VBox>
                <HBox alignment="CENTER_RIGHT" styleClass="buttonbar">
                    <children>
                        <Button fx:id="acceptButton" maxHeight="40.0" maxWidth="150.0" minHeight="40.0" minWidth="150.0"
                                mnemonicParsing="false" prefHeight="40.0" prefWidth="150.0" styleClass="button-confirm"
                                text="Aceptar" HBox.hgrow="ALWAYS"/>
                        <Button maxHeight="40.0" maxWidth="150.0" minHeight="40.0" minWidth="150.0"
                                mnemonicParsing="false" prefHeight="40.0" prefWidth="150.0" styleClass="button-cancel"
                                text="Cancelar" HBox.hgrow="ALWAYS"/>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</HBox>
