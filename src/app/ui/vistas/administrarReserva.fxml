<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="650.0" prefWidth="920.0" stylesheets="@../estilos/style.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.ui.controladores.AdministrarReservaController">
   <children>
      <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="grid-pane" HBox.hgrow="ALWAYS">
         <children>
            <TableView fx:id="tablaReservas" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
              <columns>
                <TableColumn fx:id="columnaClienteoInmueble" editable="false" maxWidth="4000.0" minWidth="-1.0" prefWidth="144.0" text="" />
                <TableColumn fx:id="columnaImporte" prefWidth="150.0" text="Importe" />
                <TableColumn fx:id="columnaFechaInicio" editable="false" prefWidth="150.0" text="Inicio de vigencia" />
                <TableColumn fx:id="columnaFechaFin" prefWidth="150.0" text="Fin de vigencia" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </TableView>
         </children>
      </VBox>
      <VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" styleClass="buttonbar">
         <children>
            <Button fx:id="botonNueva" maxHeight="1000.0" maxWidth="1000.0" mnemonicParsing="false" onAction="#agregarAction" text="Nueva" />
            <Button fx:id="botonVerPDF" maxHeight="1000.0" maxWidth="1000.0" mnemonicParsing="false" onAction="#modificarAction" text="Ver" />
            <Button fx:id="botonEliminar" maxHeight="1000.0" maxWidth="1000.0" mnemonicParsing="false" onAction="#eliminarAction" text="Eliminar" />
         </children>
      </VBox>
   </children>
</HBox>
